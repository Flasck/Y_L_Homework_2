{
  "extends": ["airbnb"],
  "ignorePatterns": [ "/node_modules/", "webpack.config.js", "statoscope.config.js" ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off",
    "no-undef": "off",
    "default-param-last": "warn"
  }
}